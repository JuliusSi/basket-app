{
    "name": "julius/basketball-courts",
    "type": "project",
    "description": "Basketball courts application.",
    "keywords": [
        "basketballcourts",
        "laravel"
    ],
    "authors": [
        {
            "name": "Julius Simanaviƒçius"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "anhskohbo/no-captcha": "^3.3",
        "beyondcode/laravel-websockets": "dev-master",
        "fruitcake/laravel-cors": "^v2.0.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "jms/serializer-bundle": "^4",
        "laravel/framework": "^9.5.0",
        "laravel/horizon": "^5.9",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "mariuzzo/laravel-js-localization": "^1.7",
        "opcodesio/log-viewer": "^1.2",
        "predis/predis": "^1.1",
        "spatie/laravel-cookie-consent": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.5.2",
        "spatie/laravel-ignition": "^1.0",
        "fzaninotto/faker": "^1.9.2",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^v6",
        "phpunit/phpunit": "9.5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.0"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Src\\": "src/",
            "Core\\": "core/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish --ansi"
        ]
    }
}
